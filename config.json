{
  "name": "Bottom Menu",
  "identifier": "bottommenu",
  "description": "Menu fixed at the bottom of your App. Specially useful when few elements shall be shown",
  "descriptionShort": "Bottomfixed menu",
  "documentation": "",
  "type": "A",
  "icon": "menu",
  "version": "1.0",
  "author": "King of App",
  "showOn": {
    "menu": false,
    "market": true,
    "dragDrop": true
  },
  "requires": [],
  "canContain": true,
  "view": "modules/koa-module-bottommenu/index.html",
  "files": [
    "modules/koa-module-bottommenu/controller.js",
    "modules/koa-module-bottommenu/styles.html"
  ],
  "libs": [
    {
      "bower": {
        "PolymerElements/iron-flex-layout": "^1.3.0"
      },
      "src": "bower_components/iron-flex-layout/iron-flex-layout.html"
    }
  ],
  "scope": {
    "path": "/bottommenu",
    "showicons": true
  },
  "config": [
    {
      "type": "checkbox",
      "key": "showicons",
      "templateOptions": {
        "description": "Show menu icons",
        "label": "Menu icons"
      }
    }
  ],
}
